CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE TABLE USERS(USERID INTEGER NOT NULL PRIMARY KEY,NAME VARCHAR(20))
CREATE TABLE EVENT(EVENTID INTEGER NOT NULL PRIMARY KEY,NAME VARCHAR(20),ORGANIZERID INTEGER NOT NULL,LOCATION VARCHAR(20),STARTTIME TIMESTAMP,ENDTIME TIMESTAMP)
CREATE TABLE INVITE(USERID INTEGER NOT NULL,EVENTID INTEGER NOT NULL,CONSTRAINT CONSTRAINT1 PRIMARY KEY(USERID,EVENTID),CONSTRAINT CONSTRAINT2 FOREIGN KEY(USERID) REFERENCES USERS(USERID),CONSTRAINT CONSTRAINT3 FOREIGN KEY(EVENTID) REFERENCES EVENT(EVENTID))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
SET SCHEMA PUBLIC
